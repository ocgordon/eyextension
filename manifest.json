{
  "manifest_version": 2,
  "name": "Eyextension",
  "description": "Your eyes are important, remember to look away for 20 seconds every 20 minutes.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Have a good day",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["content.js"]
}
